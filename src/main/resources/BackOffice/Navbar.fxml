<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10" style="-fx-padding: 10;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/24.0.1" fx:controller="edu.pidev3a32.controllers.BackNavbarController">
    <Button fx:id="btnHome" onAction="#goToHome" style="-fx-background-color: #ffce00;" text="Home" />
    <Button fx:id="btnGames" onAction="#goToGames" style="-fx-background-color: #ffce00;" text="Games" />
    <Button fx:id="btnTournaments" onAction="#goToTournaments" style="-fx-background-color: #ffce00;" text="Tournaments" />
    <Button fx:id="btnUsers" onAction="#goToUsers" style="-fx-background-color: #ffce00;" text="Users" />
    <Button fx:id="btnLogout" onAction="#logout" style="-fx-background-color: #ffce00;" text="Logout" />
</HBox>
